{
  "name": "heptagrambotproject.com",
  "version": "0.2.3",
  "packageManager": "yarn@3.2.2",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.2.1",
    "ansi-html": "0.0.9",
    "immer": "9.0.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-icons": "4.4.0",
    "set-value": "4.1.0",
    "styled-components": "5.3.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "wireit",
    "build": "wireit",
    "test": "wireit",
    "eject": "wireit",
    "prettier": "wireit",
    "prettier-check": "wireit",
    "partytown": "wireit"
  },
  "wireit": {
    "start": {
      "command": "react-scripts start"
    },
    "build": {
      "command": "react-scripts build"
    },
    "test": {
      "command": "react-scripts test"
    },
    "eject": {
      "command": "react-scripts eject"
    },
    "prettier": {
      "command": "prettier --write \"./**/*.{md,jsx,json,html,css,js,yml}\""
    },
    "prettier-check": {
      "command": "prettier --check \"./**/*.{md,jsx,json,html,css,js,yml}\""
    },
    "partytown": {
      "command": "partytown copylib public/~partytown"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "wireit": "0.7.1"
  }
}